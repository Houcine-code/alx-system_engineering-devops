#!/bin/bash
echo $(($(echo $BINARY | rev | cut -b 11)*2**10 + $(echo $BINARY | rev | cut -b 10)*2**9 + $(echo $BINARY | rev | cut -b 9)*2**8 + $(echo $BINARY | rev | cut -b 8)*2**7 + $(echo $BINARY | rev | cut -b 7)*2**6 + $(echo $BINARY | rev | cut -b 6)*2**5 + $(echo $BINARY | rev | cut -b 5)*2**4 +  $(echo $BINARY | rev | cut -b 4)*2**3+ $(echo $BINARY | rev | cut -b 3)*2**2 +$(echo $BINARY | rev | cut -b 2)*2**1 + $(echo $BINARY| rev | cut -b 1)*2**0))
